{
  "compilerOptions": {
    "outDir": ".",
    "module": "commonjs",
    "target": "es5",
    "lib": ["es6"],
    "typeRoots": ["node_modules/@types"],
    "skipLibCheck": false,
    "sourceMap": false,
    "allowJs": true,
    "moduleResolution": "node",
    "rootDir": ".",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true
  },
  "files": ["customLintRule.ts"]
}
